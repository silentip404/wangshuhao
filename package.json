    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/signale": "^1.4.7",
    "commander": "^14.0.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "signale": "^1.4.0",
